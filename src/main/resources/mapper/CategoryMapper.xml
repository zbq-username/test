<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xasxt.app.dao.CategoryMapper">
<cache eviction="FIFO" flushInterval="60000" size="124" readOnly="true" />
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xasxt.app.bean.Category">
        <id column="cateid" property="cateid" />
        <result column="cname" property="cname" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        cateid, cname
    </sql>

</mapper>
